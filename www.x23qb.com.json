{
    "name": "铅笔小说",
    "url": "www.x23qb.com",
    "version": 100,
    "search": {
        "url": "https://www.x23qb.com/search.php@post->searchkey=${key}",
        "charset": "gbk",
        "list": "#sitebox > dl",
        "name": "h3 > a",
        "author": "",
        "cover": "dt > a > img",
        "detail": "h3 > a"
    },
    "detail": {
        "name": "#bookinfo .d_title > h1",
        "author": "#bookinfo .d_title > span > a",
        "cover": "#bookimg > img",
        "summary": "#bookintro > p:nth-child(1)",
        "status": "#count > ul > li:nth-child(3) > span",
        "update": "#uptime > span",
        "lastChapter": "#newlist > ul.chaw > li:nth-child(1) > a",
        "catalog": ""
    },
    "catalog": {
        "list": "#chapterList > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a@attr->href"
    },
    "chapter": {
        "content": "#TextContent"
    }
}