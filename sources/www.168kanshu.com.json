{
    "name": "168看书网 [刺猬猫]",
    "url": "www.168kanshu.com",
    "version": 104,
    "search": {
        "url": "https://www.168kanshu.com/modules/article/search.php?action=login&searchkey=${key}",
        "charset": "gbk",
        "list": ".toplist >ul > li",
        "name": ".s1 > a",
        "author": ".s3",
        "detail": ".s1 > a"
    },
    "detail": {
        "name": "#info > h1",
        "author": "#info > .options > span.item.red",
        "cover": ".book_info > .pic > img",
        "summary": ".bookinfo_intro",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "lastChapter": "meta[property=og:novel:latest_chapter_name]@attr->content",
        "catalog": ""
    },
    "catalog": {
        "list": ".book_list > ul > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#htmlContent",
        "filter": [
            "@center"
        ],
        "page": ".chapter_Turnpage > a.next:matches(下一页)"
    }
}