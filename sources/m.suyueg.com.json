{
    "name": "速阅阁",
    "url": "m.suyueg.com",
    "version": 100,
    "search": {
        "url": "http://m.suyueg.com/s.php@post->s=${key}&type=articlename",
        "charset": "gbk",
        "list": ".sone",
        "name": ".sone > a",
        "author": ".sone > span",
        "cover": "",
        "detail": ".sone > a"
    },
    "detail": {
        "name": ".introduce > h",
        "author": ".bq > span:nth-child(2) > a",
        "cover": ".pic > img",
        "summary": ".intro > p",
        "status": "",
        "update": ".infotype > p:nth-child(3)@match->(?<=时间：)(.+)",
        "lastChapter": ".infotype > p:nth-child(4) > a",
        "catalog": ".gochapter > a@replace->m.suyueg->www.suyueg"
    },
    "catalog": {
        "list": "ul.chapters > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a@attr->href"
    },
    "chapter": {
        "content": "#articlecontent"
    }
}