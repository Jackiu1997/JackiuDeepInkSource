{
    "name": "亲小说",
    "url": "www.qinxiaoshuo.com",
    "version": 102,
    "search": {
        "url": "http://www.qinxiaoshuo.com/search/${key}/1.html",
        "list": "#main_box > div.book",
        "name": ".book_info > div > h3",
        "author": ".book_info > div > div:nth-child(2)@replace->作者: ",
        "cover": "a > img",
        "summary": ".intro",
        "detail": "a"
    },
    "detail": {
        "status": ".show_info > div > div:nth-child(2) > span:nth-child(2)",
        "update": ".show_info > div > div:nth-child(4) > span:nth-child(1)@replace->更新时间：",
        "lastChapter": ".show_info > div > div:nth-child(4) > span:nth-child(2)@replace->最新章节：",
        "catalog": ""
    },
    "catalog": {
        "list": ".volume",
        "orderBy": 0,
        "booklet": {
            "name": ".volume_title",
            "list": ".chapters > a"
        },
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#chapter_content",
        "page": "div.read_buts > a:matches(下一页)"
    }
}