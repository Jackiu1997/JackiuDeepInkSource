{
    "name": "亲小说",
    "url": "www.qinxiaoshuo.com",
    "category": 3,
    "version": 100,
    "search": {
        "url": "https://www.qinxiaoshuo.com/search/?keyword=${key}",
        "charset": "utf-8",
        "list": ".book_item",
        "name": ".item_name > a",
        "author": ".item_name > span",
        "cover": ".item_img > a > img",
        "summary": ".item_intro",
        "detail": ".item_name > a"
    },
    "detail": {
        "name": ".book_name",
        "author": ".info > a",
        "cover": "book_img@attr->src",
        "summary": ".book_intro",
        "status": ".info:nth-child(3) > span",
        "update": ".info:nth-child(4) > span",
        "lastChapter": "",
        "catalog": ""
    },
    "catalog": {
        "list": ".volume",
        "orderBy": 0,
        "booklet": {
            "name": ".volume_name > h3",
            "list": ".chapters > a"
        },
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#chapter_content",
        "page": {
            "url": ".buttons > p > a:matches(下一页)"
        }
    }
}