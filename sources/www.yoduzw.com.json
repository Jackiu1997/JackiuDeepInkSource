{
    "name": "有度中文网",
    "url": "www.yoduzw.com",
    "version": 101,
    "search": {
        "url": "https://www.yoduzw.com/search@post->searchkey=${key}",
        "charset": "gbk",
        "list": "li#hism",
        "name": "h3 > a",
        "author": "em > span:nth-child(4)",
        "cover": "a > img@attr->_src",
        "detail": "h3 > a"
    },
    "detail": {
        "summary": ".det-info > .pr > div",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "lastChapter": "meta[property=og:novel:latest_chapter_name]@attr->content",
        "catalog": ""
    },
    "catalog": {
        "list": "#chapterList > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#TextContent",
        "filter": [
            "@center:contains(本章未完)"
        ],
        "page": "#readbg > p > a:matches(下一页)"
    }
}