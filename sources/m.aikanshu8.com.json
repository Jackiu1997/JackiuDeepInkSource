{
    "name": "爱看书吧",
    "url": "m.aikanshu8.com",
    "version": 100,
    "search": {
        "url": "https://www.aikanshu8.com/modules/article/search.php?searchkey=${key}",
        "charset": "gbk",
        "list": ".mySearch > ul",
        "name": "li:nth-child(1) > a",
        "author": "li:nth-child(3)@match->(?=作者：)(.+)",
        "cover": "",
        "summary": "",
        "detail": "li:nth-child(1) > a"
    },
    "detail": {
        "name": ".bookTitle",
        "author": ".bookTag > a:nth-child(1)",
        "cover": ".panel-body > div > div > img",
        "summary": "#bookIntro",
        "status": "",
        "update": "li:nth-child(5)",
        "lastChapter": "",
        "catalog": ""
    },
    "catalog": {
        "list": "#list-chapterAll > .panel-chapterlist > dd",
        "orderBy": 0,
        "name": "a",
        "chapter": "a@attr->href"
    },
    "chapter": {
        "content": "#htmlContent"
    }
}