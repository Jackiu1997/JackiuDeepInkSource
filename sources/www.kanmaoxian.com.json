    "version": 102,
    "search": {
        "url": "https://www.kanmaoxian.com/modules/article/search.php?searchtype=articlename&searchkey=${key}",
        "charset": "gbk",
        "list": "section.list > div > ul > li",
        "name": ".title > h3@match->(?<=《)(.+)(?=》)",
        "author": ".title > span.author@replace->作者：",
        "cover": "a > img",
        "detail": "a"
    },
    "detail": {
        "name": "section.bookinfo > div.info_t > h1",
        "author": "meta[property=og:novel:author]@attr->content",
        "cover": "meta[property=og:image]@attr->content",
        "summary": "meta[property=og:description]@attr->content",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "lastChapter": "meta[property=og:novel:latest_chapter_name]@attr->content",
        "catalog": ".book_read > span.read_yd > a"
    },
    "catalog": {
        "list": "section > dl > dd",
        "orderBy": 0,
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": ".yd_text2",
        "purify": [
            "wap.kanmaoxian.com",
            "看1毛2线3中文网"
        ]
    }
