{
    "name": "读零零小说 [晋江]",
    "url": "www.du00.com",
    "version": 100,
    "search": {
        "url": "https://www.du00.com/modules/article/search.php?searchkey=${key}",
        "charset": "gbk",
        "list": "tbody > tr:gt(1)",
        "name": "td:nth-child(1) > a",
        "author": "td:nth-child(3)",
        "cover": "",
        "detail": "td:nth-child(1) > a"
    },
    "detail": {
        "name": "h1.f21h@match->(.+)(?=作者:)",
        "author": "h1.f21h > em@replace->作者:",
        "cover": ".box_intro > .pic > img",
        "summary": ".intro",
        "status": ".box_info > table > tbody > tr:nth-child(5) > td:nth-child(2)@match->(?<=状态：)(.+)",
        "update": "tbody > tr:nth-child(6) > td:nth-child(3)@match->(?<=时间：)(.+)",
        "lastChapter": "#qvod-pl-list > .play-content > p > a:nth-child(1)",
        "catalog": "span.btopt > a"
    },
    "catalog": {
        "list": "#list > dl > dd",
        "orderBy": 0,
        "name": "a",
        "id": "a@attr->href@match->(?<=\\()([0-9]{3})(?=\\))",
        "did": "a@attr->href@match->(?<=\\()([0-9]{5})(?=\\))",
        "qid": "a@attr->href@match->(?<=\\()([0-9]{8})(?=\\))",
        "chapter": "https://www.du00.com/t/t.php?id=/${$catolog@match->(?<=\\()([0-9]{3})(?=\\))}&did=${a@attr->href}&qid=${a@attr->href}&page=${a@attr->href}"
    },
    "chapter": {
        "content": "#pagecontent"
    }
}